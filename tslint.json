{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "max-classes-per-file": false,
        "no-empty": false,
        "no-null-keyword": true,
        "object-literal-sort-keys": false,
        "whitespace": {
            "options": [
                "check-branch",
                "check-decl",
                "check-module",
                "check-operator",
                "check-separator",
                "check-type",
                "check-typecast"
            ]
        }
    },
    "rulesDirectory": []
}
